<h3></h3>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Form
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="alert alert-danger" role="alert">
                            <ul>
                                <li>Name: không được rỗng</li>
                                <li>Status: chọn một trạng thái</li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <form role="form" action="<%= systemConfig.prefixAdmin %>/salary/edit/<%= dataSalary.id %>" method="post">
                            <div class="form-group">
                                <label>Name</label>
                                <input class="form-control" type="number" name="wage" value="<%= dataSalary.wage %>">
                            </div>
                            <div class="form-group">
                                <label>Birthday</label>
                                <input class="form-control" type="basic_salary" name="basic_salary"value="<%= dataSalary.basic_salary %>">
                            </div>
                            <div class="form-group">
                                <label>Born</label>
                                <input class="form-control" type="number" name="coefficient_salary" value="<%= dataSalary.coefficient_salary %>">
                            </div>
                            <div class="form-group">
                                <label>Born</label>
                                <input class="form-control" type="number" name="allowance_coefficient" value="<%= dataSalary.allowance_coefficient %>">
                            </div>
                            <!-- <div class="form-group">
                                <label>Content</label>
                                <textarea class="form-control" name="content" rows="2"  id="content_ck" ></textarea>
                            </div> -->
                            <input class="form-control" name="id" value="" type="hidden">
                            <button type="submit" class="btn btn-primary">Submit </button>
                            <a type="button" href="<%= systemConfig.prefixAdmin %>/employees"
                                class="btn btn-default">Cancel</a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>